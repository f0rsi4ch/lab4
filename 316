user_input = input("Введіть натуральне число: ")

try:
    number = int(user_input)
except ValueError:
    print("Помилка: Введіть ціле число.")
    exit()

if number <= 0:
    print("Помилка: Число повинно бути натуральним.")
    exit()

s = str(number)

if len(s) <= 1:
    result = number
else:
    first_digit = s[0]
    last_digit = s[-1]
    middle_part = s[1:-1]

    new_s = last_digit + middle_part + first_digit

    result = int(new_s)

print(f"Результат: {result}")
